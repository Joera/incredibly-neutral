#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joera/Documents/incredibly-neutral/website/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules/rabin-wasm/cli/node_modules:/home/joera/Documents/incredibly-neutral/website/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules/rabin-wasm/node_modules:/home/joera/Documents/incredibly-neutral/website/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules:/home/joera/Documents/incredibly-neutral/website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joera/Documents/incredibly-neutral/website/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules/rabin-wasm/cli/node_modules:/home/joera/Documents/incredibly-neutral/website/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules/rabin-wasm/node_modules:/home/joera/Documents/incredibly-neutral/website/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules:/home/joera/Documents/incredibly-neutral/website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli/bin.js" "$@"
else
  exec node  "$basedir/../../cli/bin.js" "$@"
fi
